{
    "name": "chewyou/silverstripe-clean-login-screen",
    "description": "Clean login screen",
    "keywords": [
        "silverstripe",
        "login"
    ],
    "homepage": "https://github.com/chewyou/silverstripe-node-tree-dropdown",
    "authors": [
        {
            "name": "Ben Pickett",
            "email": "app.benspickett@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "silverstripe-vendormodule",
    "autoload": {
        "psr-4": {
            "Chewyou\\LoginScreen\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/css",
            "client/js",
            "client/vendor"
        ]
    },
    "require": {
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
